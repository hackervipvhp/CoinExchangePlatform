        if(val.match(/[a-z]/)){
            strenth++;
        }
        if(val.match(/[A-Z]/)){
            strenth++;
        }
        if(val.match(/[0-9]/)){
            strenth++;
        }
        if(val.match(/\W/) ){
            strenth++;
        }
        switch(strenth){
            case 4:
                password_result.html('Strong');
