                    {
                        "border-bottom-color":"#FF9C33",
                    }
                );
                break;
            case 2:
                password_result.html('Weak');
                password_result.css(
                    {
                        "color" : "#FA0606"
                    }
                );
                password.css(
                    {
                        "border-bottom-color":"#FA0606",
                    }
                );
                break;
